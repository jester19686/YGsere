# Server Configuration
PORT=4000
NODE_ENV=production

# CORS & Frontend
FRONT_ORIGIN=http://localhost:3000,https://yourdomain.com

# Telegram Bot (Optional - for auth bot)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_API_ROOT=https://api.telegram.org
APP_BASE_URL=http://localhost:4000

# Proxy (Optional - if using proxy for Telegram API)
# HTTPS_PROXY=http://proxy.example.com:port
# HTTP_PROXY=http://proxy.example.com:port

# Rate Limiting
RATE_ROOMS_WINDOW_MS=60000
RATE_ROOMS_MAX=10
AUTH_RATE_WINDOW_MS=60000
AUTH_RATE_MAX=20

